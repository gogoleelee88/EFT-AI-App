# EFT AI PWA 환경변수 설정

# AI 서버 설정 (FastAPI 백엔드)
VITE_AI_SERVER_URL=http://localhost:8000

# Firebase 설정
# Firebase 콘솔 (https://console.firebase.google.com)에서 값을 가져와 .env 파일에 설정하세요
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# 개발용 임시 설정 (실제로는 Firebase 프로젝트 생성 후 값 변경 필요)
# VITE_FIREBASE_API_KEY=AIzaSyDemoKey123456789
# VITE_FIREBASE_AUTH_DOMAIN=eft-ai-app-demo.firebaseapp.com
# VITE_FIREBASE_PROJECT_ID=eft-ai-app-demo
# VITE_FIREBASE_STORAGE_BUCKET=eft-ai-app-demo.appspot.com
# VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
# VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456

# 앱 설정
VITE_APP_NAME=EFT AI 마음돌봄
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# 개발/디버그 설정
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info
VITE_SHOW_AI_METADATA=true

# PWA 설정
VITE_PWA_ENABLED=true
VITE_OFFLINE_SUPPORT=true

# 분석 도구 (선택사항)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_SENTRY_DSN=https://your-sentry-dsn